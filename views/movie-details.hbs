<button><a href="/movies">Go Back</a></button>
<div>
    <img src= "{{movie.image}}">
    <h3>{{movie.title}}</h3>
    <h4>Director: {{movie.director}}</h4>
<ul>
    {{#each movie.stars}}
    <li>{{this}}</li>
    {{/each}}
</ul>
<p>{{movie.description}}</p>
<ul>
    {{#each movie.showtimes}}
    <li>{{this}}</li>
    {{/each}}
</ul>
</div>